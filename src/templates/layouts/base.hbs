<!doctype html>
<html class="no-js core no-flexbox">
<head>
    <meta charset="utf-8">
    <title>{{ title }}</title>
    {{#if description}}<meta name="description" content="{{description}}"/>{{/if}}
    {{#is type "home" }}<style>{{> homeheadstyles}}</style>{{/is}}
    {{#is type "page" }}<link href="{{assets}}/prefixed/style.min.css" rel="stylesheet">{{/is}}
    {{#is type "page-new" }}<link href="{{assets}}/prefixed/style-2015.min.css" rel="stylesheet">{{/is}}
    <!-- {{#is theme "gallery" }}<link href="{{assets}}/prefixed/gallery.min.css" rel="stylesheet">{{/is}} -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="author" href="https://plus.google.com/+BramdeHaan/posts">

<link rel="apple-touch-icon" sizes="57x57" href="/apple-touch-icon-57x57.png">
<link rel="apple-touch-icon" sizes="60x60" href="/apple-touch-icon-60x60.png">
<link rel="apple-touch-icon" sizes="72x72" href="/apple-touch-icon-72x72.png">
<link rel="apple-touch-icon" sizes="76x76" href="/apple-touch-icon-76x76.png">
<link rel="apple-touch-icon" sizes="114x114" href="/apple-touch-icon-114x114.png">
<link rel="apple-touch-icon" sizes="120x120" href="/apple-touch-icon-120x120.png">
<link rel="apple-touch-icon" sizes="144x144" href="/apple-touch-icon-144x144.png">
<link rel="apple-touch-icon" sizes="152x152" href="/apple-touch-icon-152x152.png">
<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon-180x180.png">
<link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
<link rel="icon" type="image/png" href="/android-chrome-192x192.png" sizes="192x192">
<link rel="icon" type="image/png" href="/favicon-96x96.png" sizes="96x96">
<link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
<link rel="manifest" href="/manifest.json">
<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#2f2f23">
<meta name="msapplication-TileColor" content="#da532c">
<meta name="msapplication-TileImage" content="/mstile-144x144.png">
<meta name="theme-color" content="#e2d112">

<script>document.documentElement.className = document.documentElement.className.replace(/\bno-js\b/g, '') + ' js '; if ( 'querySelector' in document && 'addEventListener' in window && Array.prototype.forEach ) { document.documentElement.className = document.documentElement.className.replace(/\bcore\b/g, '') + ' enhanced '; } var s = document.body || document.documentElement, s = s.style; if( s.webkitFlexWrap == '' || s.msFlexWrap == '' || s.flexWrap == '' ) { document.documentElement.className = document.documentElement.className.replace(/\bno-flexbox\b/g, '') + ' flexbox '; }
</script>
</head>

{{#is theme "gallery" }}<body class="gallery">{{/is}}
{{#is theme "article" }}<body class="article">{{/is}}
{{#is theme "index" }}<body class="index">{{/is}}
{{#is theme "info" }}<body class="info">{{/is}}
{{> body }}
{{#is page-id "contact" }}<script>{{> wufoo-embed-js}}</script>{{/is}}
{{#is type "home" }}<script>{{> homepage-embed-js}}</script>{{/is}}
{{#is type "page" }}<script src="{{assets}}/js/main.min.js"></script>{{/is}}
</body>
</html>
